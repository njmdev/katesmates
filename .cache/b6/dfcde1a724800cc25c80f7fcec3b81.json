:#fff;\r\n        font-size:1.15rem;\r\n        &:hover{\r\n            background: #41737b;\r\n            color:#fff;\r\n        }\r\n    }\r\n}\r\n.hero{\r\n    //background-image: url(\"../img/dogwalk-forest.jpg\");\r\n    background-color:#205c72;\r\n    background-size: cover;\r\n    background-position: center;\r\n    box-shadow: inset 0 0 0 2000px rgba(0, 0, 0, 0.6);\r\n    .hero-inner{\r\n        display:flex;\r\n        align-items: center;\r\n        justify-content: space-evenly;\r\n        &__right{\r\n            padding: 0 5rem;\r\n            .content{\r\n                h1{\r\n                    font-size:4rem;\r\n                }\r\n                h1, p{\r\n                    color:#fff;\r\n                }\r\n            }\r\n            .social-container{\r\n                display: flex;\r\n                align-items: center;\r\n                justify-content: space-between;\r\n                &__links{\r\n                    display: flex;\r\n                    align-items: center;\r\n                    span{\r\n                        color:#fff;\r\n                    }\r\n                    .fa-facebook, .fa-instagram{\r\n                        font-size:1.8rem;\r\n                        margin-left: 1rem;\r\n                        color:#fff;\r\n                    }\r\n                } \r\n            }   \r\n        }\r\n    }\r\n}\r\n\r\n.card{\r\n    .card-image{\r\n        height:285px;\r\n        width:100%;\r\n        background-size: cover;\r\n        background-position: center;\r\n        &--first{\r\n            background-image: url(\"../img/whippetmedium.jpg\");\r\n        }\r\n        &--second{\r\n            background-image: url(\"../img/cat.jpg\");\r\n        }\r\n        &--third{\r\n            background-image: url(\"../img/senior.jpg\");\r\n        }\r\n    }\r\n    &-content{\r\n        &__info{\r\n            min-height:17rem;\r\n        }\r\n        &__pricing{\r\n            display:flex;\r\n         